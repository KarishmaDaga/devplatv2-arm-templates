{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspaceName": {
      "type": "String"
    },
    "environmentName": {
      "defaultValue": "testEnvironmentContainerName",
      "type": "String"
    },
    "environmentDescription": {
      "defaultValue": "This is a test description for an environment container created from an ARM template",
      "type": "String"
    },
    "condaFile": {
      "type": "String"
    },
    "dockerProperties": {
      "defaultValue": {
        "platform": {
          "operatingSystemType": "string"
        },
        "dockerSpecificationType": "Build",
        "context": "string",
        "dockerfile": "string",
        "dockerImageUri": "string"
      },
      "type": "Object"
    },
    "inferenceContainerProperties": {
      "defaultValue": {},
      "type": "Object"
    },
    "environmentProperties": {
      "defaultValue": {
        "condaFile": "string",
        "description": "[parameters('environmentContainerDescription')]",
        "isAnonymous": "boolean",
        "properties": {},
        "tags": {}
      },
      "type": "Object"
    }
  },
  "resources": [
    {
      "name": "[concat(parameters('workspaceName'), '/', parameters('environmentContainerName'), '/', parameters('environmentName'))]",
      "type": "Microsoft.MachineLearningServices/workspaces/environments/versions",
      "apiVersion": "2021-03-01-preview",
      "properties": {
        "condaFile": "string",
        "description": "string",
        "docker": "[parameters('dockerProperties')]",
        "isAnonymous": "boolean",
        "properties": {},
        "tags": {}
      }
    }
  ]
}