{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspaceName": {
      "type": "String"
    },
    "dataContainerName": {
      "defaultValue": "testEnvironmentContainerName",
      "type": "String"
    },
    "dataContainerDescription": {
      "defaultValue": "This is a test description for an environment container created from an ARM template",
      "type": "String"
    }
  },
  "resources": [
    {
      "type": "Microsoft.MachineLearningServices/workspaces/data",
      "apiVersion": "2021-03-01-preview",
      "name": "[concat(parameters('workspaceName'), '/', parameters('dataContainerName'))]",
      "properties": {
        "description": "[parameters('dataContainerDescription')]",
        "properties": {},
        "tags": {}
      }
    }
  ]
}