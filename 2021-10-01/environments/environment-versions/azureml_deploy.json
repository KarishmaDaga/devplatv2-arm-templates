{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspaceName": {
      "type": "String"
    },
    "environmentAssetName": {
      "type": "String",
	  "defaultValue": "testEnvironmentAssetGA"
    },
    "environmentAssetVersion": {
      "defaultValue": "1",
      "type": "String"
    },
    "environmentDescription": {
      "defaultValue": "This is a test description for an environment created from an ARM template",
      "type": "String"
    },
	"dockerContextURI": {
		"type": "string",
		"defaultvalue": "",
		"metadata": {
			"description": "Docker build context URI, for example: https://storage-account.blob.core.windows.net/azureml/DockerBuildContext/95ddede6b9b8c4e90472db3acd0a8d28/"
		}
	},
	"dockerFilePath": {
		"type": "string",
		"defaultValue": "",
		"metadata": {
			"description": "Docker file path, for example: prod/Dockerfile"
		}
	},
    "condaFile": {
      "defaultValue": "",
      "type": "String",
      "metadata": {
        "description": "Standard configuration file used by Conda that lets you install any kind of package, including Python, R, and C/C++ packages."
      }
    },
    "isAnonymous": {
      "defaultValue": false,
      "type": "Bool"
    },
	"environmentType": {
		"type": "string",
		"allowedValues": [
			"Curated"
		]
	},
    "dockerImage": {
      "defaultValue": "",
      "type": "String",
      "metadata": {
        "description": "Docker image path, for example: 'docker.io/tensorflow/serving:latest'."
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.MachineLearningServices/workspaces/environments/versions",
      "apiVersion": "2021-10-01",
      "name": "[concat(parameters('workspaceName'), '/', parameters('environmentAssetName'), '/', parameters('environmentAssetVersion'))]",
      "properties": {
		"build": {
			"contextUri": "[parameters('dockerContextURI')]",
			"dockerfilePath": "[parameters('dockerfilePath')]"
		},
		"condaFile": "[parameters('condaFile')]",
		"description": "[parameters('environmentDescription')]",
		"environmentType": "[parameters('environmentType')]",
		"image": "[parameters('dockerImage')]",
		"inferenceConfig": {},
        "isAnonymous": "[parameters('isAnonymous')]",
        "properties": {},
        "tags": {}
      }
    }
  ]
}
